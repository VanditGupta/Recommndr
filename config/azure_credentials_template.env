# Azure Configuration Template
# Copy this file to .env and fill in your actual values

# Azure Configuration
AZURE_RESOURCE_GROUP=recommndr-rg
AZURE_LOCATION=eastus

# Storage Account
AZURE_STORAGE_ACCOUNT_NAME=recommndrstorage
AZURE_STORAGE_ACCOUNT_KEY=YOUR_STORAGE_ACCOUNT_KEY_HERE
AZURE_STORAGE_CONNECTION_STRING=DefaultEndpointsProtocol=https;AccountName=recommndrstorage;AccountKey=YOUR_STORAGE_ACCOUNT_KEY_HERE;EndpointSuffix=core.windows.net

# Container Registry
AZURE_ACR_NAME=recommndracr
AZURE_ACR_LOGIN_SERVER=recommndracr.azurecr.io
AZURE_ACR_USERNAME=recommndracr
AZURE_ACR_PASSWORD=YOUR_ACR_PASSWORD_HERE

# Key Vault
AZURE_KEY_VAULT_NAME=recommndr-kv
AZURE_KEY_VAULT_URI=https://recommndr-kv.vault.azure.net/

# ML Workspace
AZURE_ML_WORKSPACE_NAME=recommndr-ml
AZURE_MLFLOW_TRACKING_URI=azureml://eastus.api.azureml.ms/mlflow/v1.0/subscriptions/3099dd32-fa0c-4404-ae13-4198614916f3/resourceGroups/recommndr-rg/providers/Microsoft.MachineLearningServices/workspaces/recommndr-ml

# Storage Containers
AZURE_DATA_CONTAINER=data
AZURE_MODELS_CONTAINER=models

# Subscription
AZURE_SUBSCRIPTION_ID=3099dd32-fa0c-4404-ae13-4198614916f3
AZURE_TENANT_ID=a8eec281-aaa3-4dae-ac9b-9a398b9215e7

# Project Configuration
